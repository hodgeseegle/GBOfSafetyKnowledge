                android:value="nur:ic_launcher,pre:ic_launcher" />
            <meta-data
                android:name="AppTab"
                android:value="Preschool/baibaoxiang" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
